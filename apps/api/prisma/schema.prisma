

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  
}

model User {
  id         Int      @id @default(autoincrement())
  telegramId Int      @unique
  username   String?
  firstName  String?
  lastName   String?
  createdAt  DateTime @default(now())
}